          "name": "Create Process",
          "event": [
            {
              "listen": "test",
              "script": {
                "exec": [
                  "if (pm.response.code === 201) {",
                  "    const response = pm.response.json();",
                  "    pm.collectionVariables.set('processId', response.id);",
                  "    pm.test('Process created successfully', () => {",
                  "        pm.expect(response.id).to.exist;",
                  "        pm.expect(response.name).to.equal('Test Process');",
                  "    });",
                  "}"
                ]
              }
            }
          ],
          "request": {
            "method": "POST",
            "header": [
              {
