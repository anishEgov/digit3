            "body": {
              "mode": "raw",
              "raw": "{\n  \"name\": \"Test Process\",\n  \"code\": \"TEST_PROCESS\",\n  \"description\": \"A test process for API validation\",\n  \"version\": \"1.0\",\n  \"sla\": 86400\n}"
            },
